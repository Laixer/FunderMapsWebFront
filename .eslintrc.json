{
  "env": {
    "browser": true,
    "es2022": true
  },
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "quotes": [
      1,
      "double",
      {
        "avoidEscape": true
      }
    ]
  }
}
